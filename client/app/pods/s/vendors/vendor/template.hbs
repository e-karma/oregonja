<h1>Vendor Template</h1>
<div class="vendor-template">
  <div class="caption">
    <label for="">Company:</label><span>{{model.name}}</span>
    <label for="">Description:</label><span>{{model.description}}</span>
    <label for="">Address:</label><span>{{model.address}}</span>
    <label for="">City:</label><span>{{model.city}}</span>
    <label for="">State:</label><span>{{model.state}}</span>
    <label for="">Zip:</label><span>{{model.zip}}</span>
    <label for="">Phone:</label><span>{{model.phone}}</span>
    <label for="">Email:</label><span>{{model.email}}</span>

  </div>
  {{link-to 'Create Event' 's.events.create' vendor=vendor.id}}
  {{link-to 'Create Employee' 's.vendors.vendor.employees.create' vendor=vendor.id}}
  {{link-to 'Create Task' 's.vendors.vendor.tasks.create' vendor=vendor.id}}
  {{link-to 'Create Opportunity' 's.vendors.vendor.opportunities.create' vendor=vendor.id}}


</div>


<div class="btn-group btn-group-justified" role="group" aria-label="...">
  <div class="btn-group" role="group">
{{#link-to 's.vendors.vendor.employees'}}    <button type="button" class="btn btn-default">Employees</button>{{/link-to}}
  </div>
  <div class="btn-group" role="group">
{{#link-to 's.vendors.vendor.tasks'}}    <button type="button" class="btn btn-default">Tasks</button>{{/link-to}}
  </div>
  <div class="btn-group" role="group">
    {{#link-to 's.vendors.vendor.opportunities'}}<button type="button" class="btn btn-default">Opportunities</button>{{/link-to}}
  </div>
</div>

{{liquid-outlet}}
