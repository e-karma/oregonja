<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Role</th>
      <th>Title</th>
      <th>Phone</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each model.employees as |employee|}}
    <tr>
      <td>{{#link-to 's.vendors.vendor.employees.employee' employee}}{{employee.fullName}}{{/link-to}}</td>
      <td>{{employee.email}}</td>
      <td>{{employee.role}}</td>
      <td>{{employee.title}}</td>
      <td>{{employee.phone}}</td>
      <td><button type="button" class="btn btn-danger" {{action 'deleteEmployee' employee}}>Delete</button></td>
    </tr>
    {{/each}}
  </tbody>
</table>
