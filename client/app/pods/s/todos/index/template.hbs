<div class="row">
	<div class="col-md-3">
			<div class="list-group">
				{{#each model as |todo|}}

			  {{#link-to 's.todos.todo' todo.id class="list-group-item"}}
			    <h4 class="list-group-item-heading">{{todo.title}}</h4>
			    <p class="list-group-item-text">{{todo.body}}</p>
			  {{/link-to}}
				{{/each}}

			</div>
	</div>
	<div class="col-md-9">

	</div>

</div>
