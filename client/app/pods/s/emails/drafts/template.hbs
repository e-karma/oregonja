<table class="table table-hover">
  <thead>
    <tr>
      <th>From</th>
      <th>Subject</th>
      <th>To</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |email|}}
    <tr>
      <td>{{email.from}}</td>
      <td>{{#link-to 's.emails.email' email}}{{email.title}}{{/link-to}}</td>
      <td>{{email.to}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

<h1>Drafts</h1>
{{outlet}}
